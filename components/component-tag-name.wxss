.check{
  position: relative;
  background: #272727;
}
.check::after {
  content:"";
  position:absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 5vh;
  line-height: 5vh;
  border-radius: 4px;
  border: 2px solid #F09338;
  background: linear-gradient(180deg, rgba(240, 147, 56, 0) 40.32%, rgba(240, 147, 56, 0.3) 99.35%);
}
